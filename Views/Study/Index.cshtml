@{
    ViewData["Title"] = "Study Page";
}
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<link rel="stylesheet" href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.css" />
<script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.js"></script>
<script language="javascript" type="text/javascript" src="~/js/Script/Study/Study.js"></script>

<!-- begin :: Content Header -->
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="page-header">
                <h1 class="text-warning mt-4" style="text-align:center">Study</h1>
            </div>
            <!-- begin :: Datatable -->
            <div class="bs-component">
                <div class="col-sm-12">
                    <table class="table table-striped table-bordered dataTable" id="tblStudy" style="width:100%">
                        <thead>
                            <tr role="row">
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b>Study Id</b></th>
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b>Version ID</b></th>
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b>Protocol Title</b></th>
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b>Protocol Code</b></th>
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b>IsActive</b></th>
                                <th class="sorting_asc" rowspan="1" colspan="1" style="color: #ffffff;"><b></b></th>
                            </tr>
                        </thead>
                        <tbody id="bodyTask"></tbody>
                    </table>
                </div>
            </div>
            <!-- end :: Datatable -->
        </div>
    </div>
</div>
<!-- end :: Content Header -->
